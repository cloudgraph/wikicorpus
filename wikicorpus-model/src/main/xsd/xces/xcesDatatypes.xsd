                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element name="label" type="xces:phrase.seq"/>
                        <xs:element name="item" type="xces:itemType"/>
                        <xs:element name="list" type="xces:listType"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="measureType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">A number, word, phrase indicating a
            quantity.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:base.seq">
                <xs:attribute name="type">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="weight"/>
                            <xs:enumeration value="length"/>
                            <xs:enumeration value="count"/>
                            <xs:enumeration value="area"/>
                            <xs:enumeration value="volume"/>
                            <xs:enumeration value="currency"/>
                            <xs:enumeration value="temperature"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="value" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!--
	<xs:complexType name="mentionedType" mixed="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">Words or phrases mentioned, not used.</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="xces:phrase.mentioned.seq"/>
		</xs:sequence>
		<xs:attributeGroup ref="xces:textAtts"/>
	</xs:complexType>
	-->
    <xs:complexType name="nameType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">A proper noun or noun phrase. </xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
        	<!-- 0.4 Changed to extend xces:phrase.seq rather than xces:base.seq. 
            <xs:extension base="xces:base.seq">
        	-->
        	<xs:extension base="xces:phrase.seq">
                <xs:attribute name="type" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!--
	<xs:group name="noteGroup">
		<xs:choice>
			<xs:group ref="xces:m.subpar.seq"/>
			<xs:group ref="xces:m.par.seq"/>
		</xs:choice>
	</xs:group>
	-->
    <xs:complexType name="noteType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">Any form of note, usually a footnote. This tag marks
                only notes that are a part of the original text, not notes that may be added by the
                encoder, etc.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:class.text">
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="xces:m.subpar.seq"/>
                    <xs:element name="p" type="xces:pType"/>
                    <xs:element name="sp" type="xces:spType"/>
                </xs:choice>
                <xs:attribute name="place" default="unspec">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="side"/>
                            <xs:enumeration value="foot"/>
                            <xs:enumeration value="end"/>
                            <xs:enumeration value="unspec"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="numType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">A number, written in any form.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:class.textString">
                <xs:attribute name="type" type="xs:string"/>
                <xs:attribute name="value" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="openerType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">Groups together any opening material that is not a
                heading at the start of a division, including in particular &lt;dateline&gt;
                and &lt;keywords&gt;. </xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:class.text">
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="xces:m.phrase.seq"/>
                    <xs:element name="dateline" type="xces:datelineType"/>
                    <xs:element name="keywords" type="xces:keywordsType"/>
                    <!-- 0.4 Changed m.subpar.seq to m.phrase.seq -->
                </xs:choice>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="pType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">A paragraph in a written text.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:subpar.seq"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="poemType" mixed="false">
        <xs:annotation>
            <xs:documentation xml:lang="en">A poem, or an extract from one, embedded or quoted
                within a text. </xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
            <xs:extension base="xces:class.text.elementOnly">
                <xs:sequence>
                    <xs:element name="head" type="xces:headType" minOccurs="0"/>
                    <xs:choice maxOccurs="unbounded">
                        <xs:element name="lg" type="xces:lgType"/>
                        <xs:group ref="xces:line.group"/>
                        <!-- 0.4
                                                        Change the name of the line element back to l (lowercase el)
                                                <xs:element name="line" type="xces:lineType"/>
                                                 -->
                    </xs:choice>
                    <xs:element name="author" type="xces:authorType" minOccurs="0"
                        maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ptrType">
        <xs:annotation>
            <xs:documentation xml:lang="en">A pointer to another location in the current document in
                terms of one or more identifiable elements. </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="xces:class.text.elementOnly">
                <xs:attribute name="corresp" type="xs:IDREFS"/>
                <xs:attribute name="next" type="xs:IDREF"/>
                <xs:attribute name="prev" type="xs:IDREF"/>
                <xs:attribute name="type" type="xs:string"/>
                <xs:attribute name="resp" type="xs:string"/>
                <xs:attribute name="crdate" type="xs:string"/>
                <xs:attribute name="targType" type="xs:NMTOKENS"/>
                <xs:attribute name="targOrder" default="u">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="y"/>
                            <xs:enumeration value="n"/>
                            <xs:enumeration value="u"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="evaluate">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="all"/>
                            <xs:enumeration value="one"/>
                            <xs:enumeration value="none"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="target" type="xs:IDREFS" use="required"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="qType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">Quoted dialogue or other quoted material appearing
                inside a paragraph. </xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:class.text">
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:group ref="xces:m.subpar.seq"/>
                    <xs:element name="p" type="xces:pType"/>
                </xs:choice>
                <xs:attribute name="next" type="xs:IDREF"/>
                <xs:attribute name="prev" type="xs:IDREF"/>
                <xs:attribute name="type" type="xs:string"/>
                <xs:attribute name="direct" default="unspecified">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="y"/>
                            <xs:enumeration value="n"/>
                            <xs:enumeration value="unspecified"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="who" type="xs:string"/>
                <xs:attribute name="broken" default="no">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="yes"/>
                            <xs:enumeration value="no"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!--
	<xs:complexType name="quoteType" mixed="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">a quotation from some author other than that of the surrounding text, usually either embedded or displayed. </xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="true">
			<xs:extension base="xces:class.text">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:group ref="xces:m.phrase.hi.seq"/>
					<xs:element name="p" type="xces:subpar.seq"/>
				</xs:choice>
				<xs:attribute name="type" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	-->
    <xs:complexType name="quoteType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">a quotation from some author other than that of the
                surrounding text, usually either embedded or displayed. </xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
        	<!-- 0.4 extend par.seq instead of subpar.seq -->
            <xs:extension base="xces:par.seq">
            	<!--
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="p" type="xces:subpar.seq"/>
                    <xs:element name="poem" type="xces:poemType"/>
                    <xs:element name="quote" type="xces:quoteType"/>
                </xs:choice>
            	-->
                <xs:attribute name="type" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="authorType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">Author of a quote or poem in the
            text.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:class.text">
                <xs:choice>
                    <xs:group ref="xces:m.phrase.seq"/>
                </xs:choice>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="refType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">A reference to another location in the current document,
                in terms of one or more identifiable elements, possibly modified by additional text
                or comment.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:subpar.seq">
                <xs:attribute name="corresp" type="xs:IDREFS"/>
                <xs:attribute name="next" type="xs:IDREF"/>
                <xs:attribute name="prev" type="xs:IDREF"/>
                <xs:attribute name="type" type="xs:string"/>
                <xs:attribute name="resp" type="xs:string"/>
                <xs:attribute name="crdate" type="xs:string"/>
                <xs:attribute name="targType" type="xs:NMTOKENS"/>
                <xs:attribute name="targOrder" default="u">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="y"/>
                            <xs:enumeration value="n"/>
                            <xs:enumeration value="u"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="evaluate">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="all"/>
                            <xs:enumeration value="one"/>
                            <xs:enumeration value="none"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="target" type="xs:IDREFS"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="regType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">Text which has been regularized or normalized in some
                sense.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:phrase.seq">
                <xs:attribute name="orig" type="xs:string"/>
                <xs:attribute name="resp" type="xs:string"/>
                <xs:attribute name="cert" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="rowType">
        <xs:complexContent>
            <xs:extension base="xces:class.text.elementOnly">
                <xs:choice maxOccurs="unbounded">
                    <xs:element name="cell" type="xces:cellType"/>
                    <xs:element name="table" type="xces:tableType"/>
                </xs:choice>
                <xs:attribute name="role" type="xs:string" default="data"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="sType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">Identifies an s-unit within a document, typically an
                orthographic sentence.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:subpar.seq">
                <xs:attribute name="next" type="xs:IDREF"/>
                <xs:attribute name="prev" type="xs:IDREF"/>
                <xs:attribute name="type" type="xs:string"/>
                <xs:attribute name="broken" default="no">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="yes"/>
                            <xs:enumeration value="no"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="spType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">Material marked as "written to be spoken'' or "written
                as spoken", usually by the presence of a speaker prefix, for example in a play
                script or printed interview.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:class.text">
                <xs:choice maxOccurs="unbounded">
                    <xs:element name="speaker" type="xces:base.seq"/>
                    <xs:element name="p" type="xces:subpar.seq"/>
                    <xs:element name="stage" type="xces:stageType"/>
                </xs:choice>
                <xs:attribute name="who" type="xs:NMTOKEN"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="stageType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">Any kind of stage direction within a dramatic
            text.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:base.seq">
                <xs:attribute name="type" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="tableType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Text displayed in tabular form</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="xces:class.text.elementOnly">
                <xs:sequence>
                    <xs:element name="head" type="xces:headType" minOccurs="0"/>
                    <xs:element name="row" type="xces:rowType" maxOccurs="unbounded"/>
                </xs:sequence>
                <xs:attribute name="rows" type="xs:NMTOKEN"/>
                <xs:attribute name="cols" type="xs:NMTOKEN"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="termType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">A single-word, multi-word or symbolic designation which
                is regarded as a technical term. </xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:base.seq">
                <xs:attribute name="type" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="textType" mixed="false">
        <xs:annotation>
            <xs:documentation xml:lang="en">An individual text.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="false">
            <xs:extension base="xces:class.global">
                <xs:choice>
                    <xs:element name="body" type="xces:bodyType"/>
                    <xs:element name="group" type="xces:groupType"/>
                </xs:choice>
                <xs:attribute name="complete" default="y">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="y"/>
                            <xs:enumeration value="n"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="decls" type="xs:IDREFS"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="timeType" mixed="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">A phrase defining a time of day in any
            format.</xs:documentation>
        </xs:annotation>
        <xs:complexContent mixed="true">
            <xs:extension base="xces:base.seq">
                <xs:attribute name="ISO8601" type="xs:string"/>
                <xs:attribute name="value" type="xs:string"/>
                <xs:attribute name="type">
                    <xs:simpleType>
                        <xs:restriction base="xs:NMTOKEN">
                            <xs:enumeration value="am"/>
                            <xs:enumeration value="pm"/>
                            <xs:enumeration value="24hour"/>
                            <xs:enumeration value="descriptive"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!--
	<xs:complexType name="titleType" mixed="true">
		<xs:annotation>
			<xs:documentation xml:lang="en">The title of a work, whether article, book, journal, or series, including any alternative titles or subtitles.</xs:documentation>
		</xs:annotation>
		<xs:sequence minOccurs="0" maxOccurs="unbounded">
			<xs:group ref="xces:phrase.title.seq"/>
		</xs:sequence>
		<xs:attributeGroup ref="xces:textAtts"/>
		<xs:attribute name="type" type="xs:string"/>
	</xs:complexType>
	-->
    <!--
    
    
	    Datatypes from xcesAna.xsd v1.0
	    
	    Encoding conventions for grammatical annotations and other stand-off annotated data.	    
    -->
    <xs:attributeGroup name="a.ana">
        <xs:annotation>
            <xs:documentation xml:lang="en">Extends the globalAtts group to include type and wsd
                attributes.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="type" type="xs:string"/>
        <!-- 
        <xs:attribute name="wsd" type="xs:string"/>
        -->
        <xs:attribute name="reg" type="xs:string"/>
        <xs:attribute name="orig" type="xs:string"/>
    	<!-- 0.4 added affix attribute -->
    	<xs:attribute name="affix" type="xs:string"/>    	
    </xs:attributeGroup>
    <!-- Base class for ana elements. -->
    <xs:complexType name="class.ana" mixed="false">
        <xs:complexContent mixed="false">
            <xs:extension base="xces:class.global">
                <xs:attributeGroup ref="xces:a.ana"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="class.ana.mixed" mixed="true">
        <xs:complexContent mixed="true">
            <xs:extension base="xces:class.global.mixed">
                <xs:attributeGroup ref="xces:a.ana"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- Text elements -->
    <xs:complexType name="class.anaString">
        <xs:complexContent>
            <xs:extension base="xces:class.string">
                <xs:attributeGroup ref="xces:a.ana"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="cesAnaType">
        <xs:complexContent>
            <xs:extension base="xces:class.ana">
                <xs:sequence>
                    <xs:group ref="xces:headerGroup" minOccurs="0"/>
                    <xs:element name="chunkList" type="xces:chunkListType"/>
                </xs:sequence>
                <xs:attribute name="doc" type="xs:string"/>
                <xs:attribute name="version" type="xs:string" use="required"/>
                <xs:attribute ref="xml:base"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="chunkType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Contains a series of sentences (marked with
                &lt;s&gt; tags), a series of tokens (marked with &lt;tok&gt;tags), a
                series of paragraph-like elements marked with &lt;par&gt; tags), or "plain
                text" data (PCDATA), which is marked with &lt;data&gt; tags.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="xces:class.ana">
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element name="tok" type="xces:tokType" maxOccurs="unbounded"/>
                    <xs:element name="s" type="xces:sType"/>
                    <xs:element name="par" type="xces:parType" maxOccurs="unbounded"/>
                    <xs:element name="event" type="xces:eventType" maxOccurs="unbounded"/>
                    <xs:element name="shift" type="xces:shiftType" maxOccurs="unbounded"/>
                    <xs:element name="a" type="xces:annotationType" maxOccurs="unbounded"/>
                </xs:choice>
                <xs:attribute ref="xml:base"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="annotationType" mixed="false">
        <xs:complexContent mixed="false">
            <xs:extension base="xces:class.ana">
                <xs:sequence minOccurs="0" maxOccurs="unbounded">
                    <xs:any namespace="##other" processContents="lax"/>
                </xs:sequence>
                <xs:attribute name="from" type="xs:string"/>
                <xs:attribute name="to" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="chunkListType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Contains one or more "chunks" of
            annotation.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="xces:class.ana">
                <xs:sequence maxOccurs="unbounded">
                    <xs:element name="chunk" type="xces:chunkType"/>
                </xs:sequence>
                <xs:attribute ref="xml:base"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ctagString">
        <xs:annotation>
            <xs:documentation xml:lang="en">Contains a corpus tag, when this tag appears within the
                &lt;lex&gt; element, it gives the corpus tag associated with the
                accompanying morphosyntactic information.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="xces:class.anaString">
                <xs:attribute name="certainty" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="disambType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Groups one or more disambiguated corpus tags and/or full
                morphosyntactic descriptions associated with the token.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="xces:class.ana">
                <xs:sequence maxOccurs="unbounded">
                    <xs:element name="ctag" type="xces:ctagString"/>
                    <xs:element name="msd" type="xces:class.string" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="lexType">
        <xs:annotation>
            <xs:documentation xml:lang="en">Groups one or more alternative sets of morphosyntactic
                information associated with the token.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="xces:class.ana">
                <xs:sequence>
                    <xs:element name="base" type="xces:class.string"/>
                    <xs:element name="msd" type="xces:class.string" minOccurs="0"/>
                    <xs:element name="ctag" type="xces:ctagString"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="parType">
        <xs:annotation>
            <xs:documentation xml:lang="en">An XLink simple link that marks paragraph boundaries.
                Contains a series of &lt;tok&gt; elements, a series of &lt;s&gt;
                elements, or a series of &lt;data&gt; elements--or any inter-mixture of
                these elements.</xs:documentation>
        </xs:annotation>
